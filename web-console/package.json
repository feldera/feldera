{
  "name": "dbsp-web-console",
  "description": "Interact with the database stream processor service",
  "license": "MIT",
  "version": "0.0.1",
  "author": {
    "name": "DBSP",
    "url": "https://github.com/feldera/feldera"
  },
  "bugs": {
    "url": "https://github.com/feldera/feldera/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feldera/feldera.git"
  },
  "homepage": "https://github.com/feldera/feldera",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npx serve@latest out",
    "lint": "eslint --max-warnings 0 --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
    "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "generate-openapi": "openapi --input ../openapi.json --output ./src/lib/services/manager && yarn format && patch -p2 < openapi-fixes.patch",
    "build-openapi": "cd .. && cargo run --bin pipeline-manager -- --dump-openapi",
    "test": "PLAYWRIGHT_API_ORIGIN=http://localhost:8080/ PLAYWRIGHT_APP_ORIGIN=http://localhost:8080/ DISPLAY= yarn playwright test",
    "test-ui": "PLAYWRIGHT_API_ORIGIN=http://localhost:8080/ PLAYWRIGHT_APP_ORIGIN=http://localhost:8080/ DISPLAY= yarn playwright test --ui-host=0.0.0.0",
    "test-ct": "DISPLAY= yarn playwright test -c playwright-ct.config.ts",
    "test-ct-ui": "DISPLAY= yarn playwright test -c playwright-ct.config.ts --ui-host=0.0.0.0",
    "test-report": "yarn playwright show-report",
    "test-prepare": "git clone --depth 1 https://github.com/feldera/playwright-snapshots.git"
  },
  "//": [
    "See `yarn playwright --help` for Playwright commands",
    "When running Playwright tests via devcontainer, webkit engine doesn't work",
    "   unless DISPLAY env var is cleared with `DISPLAY= ` ",
    "   See https://github.com/microsoft/playwright/issues/18255 for details."
  ],
