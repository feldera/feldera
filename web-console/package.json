{
  "name": "web-console",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "openapi": "openapi-ts -i path/to/openapi.json -o src/client",
    "generate-openapi": "openapi-ts && bun format",
    "build-openapi": "cd .. && cargo run --bin pipeline-manager -- --dump-openapi",
    "test-e2e": "PLAYWRIGHT_API_ORIGIN=http://localhost:8080/ PLAYWRIGHT_APP_ORIGIN=http://localhost:8080/ DISPLAY= bun playwright test",
    "test-e2e-ui": "PLAYWRIGHT_API_ORIGIN=http://localhost:8080/ PLAYWRIGHT_APP_ORIGIN=http://localhost:8080/ DISPLAY= bun playwright test --ui-host=0.0.0.0",
    "test-ct": "DISPLAY= bun playwright test -c playwright-ct.config.ts",
    "test-ct-ui": "DISPLAY= bun playwright test -c playwright-ct.config.ts --ui-host=0.0.0.0",
    "test-report": "bun playwright show-report",
    "test-prepare": "git clone --depth 1 https://github.com/feldera/playwright-snapshots.git",
    "fix-icons": "oslllo-svg-fixer -s tmp/assets/icons/feldera-material-icons -d tmp/assets/icons/feldera-material-icons-fixed"
  },
  "devDependencies": {
    "@auth/sveltekit": "^1.7.3",
    "@axa-fr/oidc-client": "^7.22.32",
    "@bithero/monaco-editor-vite-plugin": "^1.0.2",
    "@fontsource-variable/dm-sans": "^5.1.0",
    "@fontsource-variable/public-sans": "^5.1.1",
    "@fontsource/dm-mono": "^5.1.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@hey-api/client-fetch": "0.1.10",
    "@hey-api/openapi-ts": "^0.47.2",
    "@playwright/test": "^1.48.2",
    "@poppanator/sveltekit-svg": "^5.0.0",
    "@revolist/svelte-datagrid": "^4.10.19",
    "@skeletonlabs/skeleton": "^3.0.0-next.9",
    "@skeletonlabs/skeleton-svelte": "^1.0.0-next.10",
    "@square/svelte-store": "^1.0.18",
    "@streamparser/json": "^0.0.21",
    "@svelte-bin/clipboard": "^0.1.4",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.12.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^8.56.12",
    "@types/node": "^20.17.6",
    "@vincjo/datatables": "^2.2.0",
    "add": "^2.0.6",
    "array-join": "^3.2.1",
    "autoprefixer": "^10.4.20",
    "bignumber.js": "^9.1.2",
    "boxicons": "^2.1.4",
    "clsx": "^2.1.1",
    "d3-format": "^3.1.0",
    "echarts": "^5.5.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "fast-deep-equal": "^3.1.3",
    "flowbite-svelte": "^0.46.23",
    "formsnap": "^2.0.0-next.1",
    "globals": "^15.12.0",
    "monaco-editor": "^0.49.0",
    "new-github-issue-url": "^1.0.0",
    "oidc-client-ts-lumeris": "^1.0.0",
    "oslllo-svg-fixer": "^5.0.0",
    "paneforge": "^0.0.5",
    "postcss": "^8.4.49",
    "posthog-js": "^1.184.2",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "pretty-ansi": "^3.0.0",
    "runed": "^0.15.3",
    "sass-embedded": "^1.80.7",
    "sk-oidc-oauth": "^1.2.0",
    "sort-on": "^6.1.0",
    "style-to-object": "^1.0.8",
    "svelte": "^5.1.16",
    "svelte-attr": "^0.0.2",
    "svelte-check": "^4.0.7",
    "svelte-echarts": "^1.0.0-rc3",
    "svelte-monaco": "^0.4.0",
    "svelte-mq-store": "^2.2.21",
    "svelte-virtual": "^1.0.0-next.22",
    "svelte-virtuallists": "^1.4.0",
    "sveltekit-superforms": "^2.20.0",
    "sveltekit-top-loader": "^0.0.4",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.14",
    "tiny-invariant": "^1.3.3",
    "true-json-bigint": "^1.0.1",
    "ts-pattern": "^5.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "valibot": "^0.36.0",
    "virtua": "^0.36.3",
    "vite": "^5.4.11",
    "vite-svg-2-webfont": "^3.4.0",
    "worker-timers": "^8.0.11"
  },
  "type": "module",
  "trustedDependencies": [
    "@axa-fr/oidc-client",
    "sk-oidc-oauth",
    "svelte-preprocess"
  ]
}
