  "homepage": "https://github.com/feldera/feldera",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npx serve@latest out",
    "lint": "eslint --max-warnings 0 --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "generate-openapi": "openapi --input ../openapi.json --output ./src/lib/services/manager",
    "test": "PLAYWRIGHT_API_ORIGIN=http://localhost:8080/ PLAYWRIGHT_APP_ORIGIN=http://localhost:8080/ DISPLAY= yarn playwright test",
    "test:ui": "PLAYWRIGHT_API_ORIGIN=http://localhost:8080/ PLAYWRIGHT_APP_ORIGIN=http://localhost:8080/ DISPLAY= yarn playwright test --ui-port=0",
    "test:report": "yarn playwright show-report",
    "test:prepare": "git clone --depth 1 https://github.com/feldera/playwright-snapshots.git"
  },
  "//": [
    "See `yarn playwright --help` for Playwright commands",
    "When running Playwright tests via devcontainer, webkit engine doesn't work",
    "   unless DISPLAY env var is cleared with `DISPLAY= ` ",
    "   See https://github.com/microsoft/playwright/issues/18255 for details."
  ],
