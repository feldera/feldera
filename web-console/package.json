    "eslint": "^9.20.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "fancy-ansi": "^0.1.3",
    "fast-deep-equal": "^3.1.3",
    "flowbite-svelte": "^0.46.23",
    "formsnap": "^2.0.0",
    "globals": "^15.14.0",
    "monaco-editor": "^0.49.0",
    "new-github-issue-url": "^1.0.0",
    "oidc-client-ts-lumeris": "^1.0.0",
    "oslllo-svg-fixer": "^5.0.0",
    "paneforge": "^0.0.5",
